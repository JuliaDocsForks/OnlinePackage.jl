using OnlinePackage

repo_name = "Test.jl"

username =  OnlinePackage.settings("username")
github_token =  OnlinePackage.settings("github_token")
appveyor_token =  OnlinePackage.settings("appveyor_token")
ssh_keygen_file =  OnlinePackage.settings("ssh_keygen_file")
set_up(username, github_token, appveyor_token, ssh_keygen_file = ssh_keygen_file)

github, travis, appveyor = generate(repo_name, github_time = 60, travis_time = 60)
OnlinePackage.delete(github)
OnlinePackage.delete(appveyor)
